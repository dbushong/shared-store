{
  "name": "shared-store",
  "version": "1.0.4",
  "description": "Keeping config data in sync",
  "main": "lib/shared-store.js",
  "author": "Jan Krems <jan.krems@groupon.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/groupon/shared-store.git"
  },
  "bugs": "https://github.com/groupon/shared-store.git",
  "license": "BSD-2-Clause",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "license": {
      "exclude": [
        "lib",
        "test"
      ]
    }
  },
  "scripts": {
    "build": "npub prep && coffeelint src test && coffee -cbo lib src",
    "watch": "coffee -wcbo lib src & nodemon -w lib -w test -x mocha",
    "pretest": "rm -rf lib && npm run build",
    "test": "mocha",
    "posttest": "npub verify",
    "setup": "rm -rf node_modules && npm install --registry ${npm_package_publishConfig_registry}",
    "release-patch": "npm run setup && npub publish patch",
    "release-minor": "npm run setup && npub publish minor",
    "release-major": "npm run setup && npub publish major"
  },
  "devDependencies": {
    "assertive": "^1.4.1",
    "coffee-script": "^1.8.0",
    "coffeelint": "^1.8.1",
    "mocha": "^2.1.0",
    "nodemon": "^1.3.7",
    "npub": "^1.0.0",
    "rimraf": "^2.2.8",
    "tmp": "0.0.24"
  },
  "dependencies": {
    "bluebird": "^2.6.2",
    "cson-parser": "^1.0.8",
    "debug": "^2.1.1",
    "deep-freeze": "0.0.1",
    "lodash": "^2.4.1",
    "mkdirp": "^0.5.0",
    "rx": "^2.3.22"
  },
  "keywords": [
    "observable",
    "reactive"
  ]
}
